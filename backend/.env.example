# Server Configuration
NODE_ENV=development
PORT=5000

# Database
MONGODB_URI=mongodb://localhost:27017/startup-benefits

# JWT Secrets (Generate strong secrets in production)
JWT_ACCESS_SECRET=your_super_secret_access_token_key_change_in_production_12345
JWT_REFRESH_SECRET=your_super_secret_refresh_token_key_change_in_production_67890

# JWT Expiration
JWT_ACCESS_EXPIRE=15m
JWT_REFRESH_EXPIRE=7d

# Client URL
CLIENT_URL=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100